    },
    {
      "title": "Training Loss",
      "type": "timeseries",
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 8},
      "targets": [
        {"expr": "lam_training_loss{loss_type=\"total\"}", "legendFormat": "Total Loss", "refId": "A"},
        {"expr": "lam_training_loss{loss_type=\"structural\"}", "legendFormat": "Structural", "refId": "B"},
        {"expr": "lam_training_loss{loss_type=\"inversion\"}", "legendFormat": "Inversion", "refId": "C"}
      ]
    },
    {
      "title": "Convergence Funnel",
      "type": "stat",
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 8},
