      ]
    },
    {
      "title": "Double-Loop Health",
      "type": "gauge",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 14},
      "targets": [{"expr": "1 - (avg(lam_surprise_magnitude) / 5)", "legendFormat": "Calibration Score", "refId": "A"}],
      "fieldConfig": {"defaults": {"min": 0, "max": 1, "thresholds": {"mode": "absolute", "steps": [{"value": 0, "color": "red"}, {"value": 0.6, "color": "yellow"}, {"value": 0.8, "color": "green"}]}}}
    },
    {
      "title": "Google Ads Sync Status",
