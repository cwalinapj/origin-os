    {
      "title": "Inversion Events by Type",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [{"expr": "sum(rate(lam_inversions_total[5m])) by (error_type)", "legendFormat": "{{error_type}}", "refId": "A"}]
